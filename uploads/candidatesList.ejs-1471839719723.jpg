<!DOCTYPE HTML>
<html>
<head>
  <charset="UTF-8">
  <title>Job Finder</title>
  <link rel="stylesheet" href="/stylesheets/style.css" type="text/css">
</head>
<body>
  <div class="header">
    <div class="clearfix">
      <div class="logo">
        <a href="/">
          <img src="/images/logo.png" alt="LOGO" height="100" width="250">
        </a>
      </div>
      <ul class="navigation">
        <li>
          <a href="/viewProfile">Profile</a>
        </li>
        <li class="active">
          <a href="/login">Logout</a>
        </li>
      </ul>
    </div>
  </div>
  <div class="contents">
    <div class="clearfix">
      <h1>Candidates List</h1>
        <table class="table-fill">
      <thead>
        <tr>
          <th class="text-left">Name</th>
          <th class="text-left">Mobile No</th>
          <th class="text-left">Location</th>
          <th class="text-left">Privilage</th>
          <th class="text-left">Work Experience</th>
          <th class="text-left">Gender</th>
          <th class="text-left">Resume</th>
        </tr>
      </thead>
      <tbody class="table-hover">
        <% for(var i=0; i< name.length; i++) { %>
        <tr>
          <td class="text-left"><%= name[i] %></td>
          <td class="text-left"><%= phno[i] %></td>
          <td class="text-left"><%= location[i] %></td>
          <td class="text-left"><%= privilage[i] %></td>
          <td class="text-left"><%= experience[i] %></td>
          <td class="text-left"><%= gender[i] %></td>
          <td class="text-left"><%= resume[i] %></td>
        </tr>
        <% } %>
      </tbody>
    </table>
    </div>
  </div>
  <div class="footer">
    <div class="clearfix">
      <div class="section contact">
        <h4>Contact Us</h4>
        <p>
          <span>Address:</span> Qburst Technologies pvt.ltd
        </p>
        <p>
          <span>Phone:</span> (+91) 9744296904
        </p>
        <p>
          <span>Email:</span> krishnadev@qburst.com
        </p>
      </div>
    </div>
  </div>
</body>
</html>